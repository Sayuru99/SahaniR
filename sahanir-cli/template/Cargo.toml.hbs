# This empty workspace definition tells Cargo to treat this project as
# a standalone entity, even if it's inside another workspace.
[workspace]

[package]
name = "{{ project_name }}"
version = "0.1.0"
edition = "2021"

[dependencies]
# When you publish your framework, you would change this to:
# sahanir = "0.1.0"
sahanir = { path = "../sahanir" }
tokio = { version = "1", features = ["full"] }
